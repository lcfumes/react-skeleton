{
  "name": "ReactJS Skeleton",
  "version": "0.0.1",
  "description": "Skeleton to starts projects with ReactJS using Webpack",
  "author": {
    "name": "Luiz Fumes",
    "email": "lcfumes@gmail.com",
    "url": "http://fumes.com.br"
  },
  "repository": "https://github.com/lcfumes/react-skeleton",
  "keywords": [
    "reactjs",
    "webpack",
    "skeleton",
    "es6"
  ],
  "license": "MIT",
  "main": "webpack.config.js",
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --config webpack.config.js",
    "dev": "NODE_ENV=dev webpack-dev-server --content-base src --inline --hot --history-api-fallback",
    "test": "NODE_ENV=test mocha './test/**/*.test.jsx' --compilers js:babel-core/register --recursive test --reporter mocha-junit-reporter --reporter-options mochaFile=./test.xml",
    "test-view": "NODE_ENV=test mocha './test/**/*.test.jsx' --compilers js:babel-core/register --recursive",
    "coverage": "NODE_ENV=test istanbul cover ./node_modules/.bin/_mocha -- test/**/*.test.jsx --compilers js:babel-core/register  --recursive"
  },
  "dependencies": {},
  "devDependencies": {
    "axios": "^0.9.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "expect": "^1.19.0",
    "expect-jsx": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.2.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.11.1",
    "mocha": "^2.4.5",
    "mocha-junit-reporter": "^1.11.1",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.2",
    "react-breadcrumbs": "^1.3.12",
    "react-dom": "^15.0.1",
    "react-keydown": "^1.6.2",
    "react-modal": "^1.2.1",
    "react-router": "^2.6.1",
    "react-select": "^1.0.0-beta14",
    "react-sortable-hoc": "0.0.8",
    "react-tagsinput": "^3.13.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
